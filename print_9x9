# -*- coding: utf-8 -*-
"""
Created on Wed Oct 28 11:51:31 2015

"""


for i in range (1,10):
    
    end = i + 1
    line = ""
    for j in range(1,end):
        answer = i * j
        #print ("1 x 1 = 1")
        temp_str = "%1d x %1d = %2d"%(i,j,answer) 
        line = line + "  " + temp_str
        
    print (line)
